# sql
CREATE TABLE students (
    id INT PRIMARY KEY AUTO_INCREMENT,
    name VARCHAR(255) NOT NULL,
    age INT,
    grade DECIMAL(4, 2)
);

INSERT INTO students (name, age, grade)
VALUES
    ('Alice', 20, 92.5),
    ('Bob', 22, 85.0),
    ('Charlie', 21, 78.3);

SELECT * FROM students
WHERE age BETWEEN 20 AND 25
AND grade >= 80;
